{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/"],
            "@app": ["src/app//*"],
            "@assets/*": ["src/assets/*"],
            "@components/*": ["src/components/*"],
            "@config": ["src/config/*"],
            "@constant": ["src/constant/*"],
            "@context/*": ["src/context/*"],
            "@hooks/*": ["src/hooks/*"],
            "@layout*": ["src/layout/*"],
            "@lib/*": ["src/lib/*"],
            "@pages/*": ["src/pages/*"],
            "@private/*": ["src/private/*"],
            "@routes/*": ["src/routes/*"],
            "@services/*": ["src/services/*"],
            "@stores/*": ["src/stores/*"],
            "@styles/*": ["src/styles/*"],
            "@utils/*": ["src/utils/*"],
            "@validations/*": ["src/validations/*"],
        }
    },
    "include": ["src"]
}